program 10 conversion
 class Main {
        public static void main(String[] args) {
            byte b;
            int i =6;
            double d=89.9099;
            System.out.println("Conversion of int to byte");
            b=(byte) i;
            System.out.println("i to b is " + i + " " +b);
            System.out.println(" Conversion of double to int");
            i=(int) d;
            System.out.println(" d to i is " + d + " " + i );
            System.out.println("Conversion  of double to byte");
            b=(byte) d;
            System.out.println("D to B is" + d + " " + b);
        }}
        
        //program 11 
class Promote {
    public static void main(String[] args) {
        byte b = 42;
        char c = 'a';
        short s = 1024;
        int i = 50000;
        float f = 5.67f;
        double d = 0.1234;

        double result = (f * b) + (i / c) - (d * s);
        System.out.println((f * b) + " + " + (i / c) + " - " + (d * s));
        System.out.println("result = " + result);
    }
}

